    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.7",
    "@types/mssql": "^6.0.4",
    "@types/uuid": "^8.3.0",
    "aws-sdk": "^2.828.0",
    "axios": "^0.19.2",
    "concurrently": "^5.3.0",
    "core-js": "^3.6.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "google-auth-library": "^6.1.3",
    "googleapis": "^39.2.0",
    "mssql": "^6.2.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "passport": "^0.4.1",
    "passport-google-oauth": "^2.0.0",
    "passport-google-oauth20": "^2.0.0",
    "register-service-worker": "^1.7.1",
    "uuid": "^8.3.0",
    "vue": "^2.6.11",
    "vue-google-auth": "^1.0.5",
    "vue-js-modal": "^2.0.0-rc.6",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
