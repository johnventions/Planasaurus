{
  "name": "planasaurus",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open --port 3355 ",
    "dev": "concurrently \"nodemon\" \"vue-cli-service serve --open --port 3355\"",
    "start": "ts-node ./server.ts",
