  "devDependencies": {
    "@types/node": "^14.0.27",
    "@vue/cli-plugin-babel": "^4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.0",
    "@vue/cli-plugin-pwa": "^4.5.0",
    "@vue/cli-service": "^4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.12.0",
    "nodemon": "^2.0.4",
    "sass-loader": "^8.0.2",
    "stylus": "^0.54.8",
    "stylus-loader": "^4.3.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "vue-template-compiler": "^2.6.11"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "server.ts",
      "server"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
